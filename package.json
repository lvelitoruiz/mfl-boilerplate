{
  "name": "ui-library-ecosystem",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "run-p start:*",
    "start:core": "cd core/ui-library && npm start",
    "start:react": "npx nx serve react-test-new",
    "start:angular": "npx nx serve angular-test",
    "start:vue": "npx nx serve vue-test",
    "start:cdn": "cd demos/cdn && python3 -m http.server 8000",
    "start:storybook": "cd core/ui-library && npm run storybook",
    "build": "cd core/ui-library && npm run build",
    "build:all": "cd core/ui-library && npm run build && npm run build:cdn",
    "setup": "cd core/ui-library && npm run build && node scripts/post-build.js",
    "test": "cd core/ui-library && npm test"
  },
  "devDependencies": {
    "@nx/angular": "^21.4.1",
    "@nx/js": "^21.4.1",
    "@nx/react": "^21.4.1",
    "@nx/storybook": "^21.4.1",
    "@nx/vite": "^21.4.1",
    "@nx/vue": "^21.4.1",
    "@nx/web": "^21.4.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/glob": "^8.1.0",
    "lit": "^2.8.0",
    "npm-run-all": "^4.1.5",
    "nx": "21.4.1",
    "rollup": "^4.49.0",
    "vite": "^6.3.5"
  },
  "overrides": {
    "@types/glob": "^8.1.0",
    "minimatch": "^9.0.3"
  },
  "resolutions": {
    "@types/glob": "^8.1.0",
    "minimatch": "^9.0.3"
  }
}
