{
  "name": "angular-test",
  "sourceRoot": "test-apps/angular-test/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "test-apps/angular-test"
      },
      "dependsOn": ["ui-library:build"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run start",
        "cwd": "test-apps/angular-test"
      },
      "dependsOn": ["ui-library:build"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test",
        "cwd": "test-apps/angular-test"
      }
    }
  },
  "tags": ["type:app", "framework:angular"],
  "implicitDependencies": ["ui-library"]
}