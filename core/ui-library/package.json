{
  "name": "ui-library",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ui-library/ui-library.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ui-library/ui-library.esm.js",
      "require": "./dist/ui-library/ui-library.cjs.js"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./dist/components/my-component.js": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./dist/components/ui-button.js": {
      "import": "./dist/components/ui-button.js",
      "types": "./dist/components/ui-button.d.ts"
    },
    "./dist/components/ui-card.js": {
      "import": "./dist/components/ui-card.js",
      "types": "./dist/components/ui-card.d.ts"
    },
    "./dist/components/ui-card-header.js": {
      "import": "./dist/components/ui-card-header.js",
      "types": "./dist/components/ui-card-header.d.ts"
    },
    "./dist/components/ui-card-title.js": {
      "import": "./dist/components/ui-card-title.js",
      "types": "./dist/components/ui-card-title.d.ts"
    },
    "./dist/components/ui-card-content.js": {
      "import": "./dist/components/ui-card-content.js",
      "types": "./dist/components/ui-card-content.d.ts"
    },
    "./dist/components/ui-input.js": {
      "import": "./dist/components/ui-input.js",
      "types": "./dist/components/ui-input.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./react/jsx": {
      "types": "./dist/react/jsx.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/components.js",
      "types": "./dist/vue/components.d.ts"
    },
    "./angular": {
      "import": "./dist/angular/components.js",
      "types": "./dist/angular/components.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    },
    "./jsx": {
      "types": "./dist/types/jsx.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil-community/postcss": "^2.2.0",
    "@stencil/angular-output-target": "^1.1.1",
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "^1.2.0",
    "@stencil/vue-output-target": "^0.11.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.3.0",
    "tailwindcss": "^3.4.17"
  },
  "license": "MIT"
}
